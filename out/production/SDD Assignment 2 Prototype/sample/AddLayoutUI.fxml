      <Label fx:id="alreadyTakenLabel" layoutX="240.0" layoutY="162.0" prefWidth="160.0" text="Name already taken" textAlignment="CENTER" textFill="RED" visible="false" />
      <Label fx:id="noSpacesLabel" layoutX="240.0" layoutY="145.0" prefWidth="160.0" text="No spaces" textAlignment="CENTER" textFill="RED" visible="false" />
      <Label fx:id="emptyLabel" layoutX="242.0" layoutY="180.0" text="Must not be empty" textFill="RED" visible="false" />
      <TextField fx:id="newLayoutBox" layoutX="240.0" layoutY="120.0" prefWidth="160.0" />
      <Button fx:id="submitButton" layoutX="293.0" layoutY="200.0" mnemonicParsing="false" onAction="#submitAnswer" text="Submit" />
   </children>
</AnchorPane>
